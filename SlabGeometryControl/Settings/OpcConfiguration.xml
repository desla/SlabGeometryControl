<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <host>localhost</host>
  <server>OPC.SimaticNET</server>
  <control>
    <activation>S7:[S7 connection_1]InLimGlobal</activation>
    <dataSize>S7:[S7 connection_1]MassiveSize</dataSize>
    <startIndex>S7:[S7 connection_1]LeftIndexGlobal</startIndex>
    <endIndex>S7:[S7 connection_1]RightIndexGlobal</endIndex>
    <times>S7:[S7 connection_1]ValueTimesGlobal</times>
    <syncDateTimeActivator>S7:[S7 connection_1]IsSyncDT</syncDateTimeActivator>
    <dateTimeForSync>S7:[S7 connection_1]DTForSync</dateTimeForSync>
    <resetToZero>S7:[S7 connection_1]ResetToZeroDb2</resetToZero>
  </control>
  <sensors>
    <sensor>
      <name>Top_0</name>      
      <enable>S7:[S7 connection_1]IsEnableSensorDb11</enable>
      <currentValue>S7:[S7 connection_1]CurrentValueDb11</currentValue>
      <sensorValuesList>S7:[S7 connection_1]SensorValuesDb11</sensorValuesList>      
    </sensor>
    <sensor>
      <name>Bottom_0</name>      
      <enable>S7:[S7 connection_1]IsEnableSensorDb12</enable>
      <currentValue>S7:[S7 connection_1]CurrentValueDb12</currentValue>
      <sensorValuesList>S7:[S7 connection_1]SensorValuesDb12</sensorValuesList>
    </sensor>
    <sensor>
      <name>Left_0</name>
      <enable>S7:[S7 connection_1]IsEnableSensorDb13</enable>
      <currentValue>S7:[S7 connection_1]CurrentValueDb13</currentValue>
      <sensorValuesList>S7:[S7 connection_1]SensorValuesDb13</sensorValuesList>
    </sensor>
    <sensor>
      <name>Right_0</name>
      <enable>S7:[S7 connection_1]IsEnableSensorDb14</enable>
      <currentValue>S7:[S7 connection_1]CurrentValueDb14</currentValue>
      <sensorValuesList>S7:[S7 connection_1]SensorValuesDb14</sensorValuesList>
    </sensor>
    <sensor>
      <name>Position</name>
      <enable>S7:[S7 connection_1]IsEnableSensorDb18</enable>
      <currentValue>S7:[S7 connection_1]CurrentValueDb18</currentValue>
      <sensorValuesList>S7:[S7 connection_1]SensorValuesDb18</sensorValuesList>
    </sensor>
  </sensors>  
</configuration>